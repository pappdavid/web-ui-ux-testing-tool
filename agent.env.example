# Agent Worker Environment Variables Template
# Copy this to .env.local and fill in your values for local testing
# For RunPod, set these in the pod environment variables

# Railway API Configuration
# Use http://localhost:3000 for local testing
# Use https://your-app.railway.app for production
RAILWAY_API_BASE_URL=http://localhost:3000

# Internal API Token (generated during deployment)
# This token is shared between Railway and RunPod
RAILWAY_INTERNAL_API_TOKEN=75f3a7ad325eb86a619c75ac1f5e3bcb8ba32225fea85958411106fb656c7250

# OpenAI Configuration
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_KEY_HERE

# OpenAI Model (optional, defaults to gpt-4o-mini)
# Options: gpt-4o-mini, gpt-4o, gpt-4-turbo
OPENAI_MODEL=gpt-4o-mini

# Playwright Browsers Path (optional)
# Use ./.playwright for local testing
# Use /ms-playwright for Docker/RunPod
PLAYWRIGHT_BROWSERS_PATH=./.playwright

# Optional: Run specific session instead of polling
# AGENT_SESSION_ID=clxxxxxxxxxxxxx

